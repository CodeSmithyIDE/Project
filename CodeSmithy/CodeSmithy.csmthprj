<?xml version="1.0"?>
<codesmithy-project-repository>
	<file-format-version>0.1.0</file-format-version>
	<name>CodeSmithyIDE</name>
	<projects>
		<codesmithy-project>
			<name>CodeSmithy</name>
			<type>CodeSmithy.Group</type>
			<description>
				<text>The CodeSmithy code and all its dependencies.</text>
			</description>
			<projects>
				<codesmithy-project>
					<name>pugixml</name>
					<type>CodeSmithy.Group</type>
					<description>
						<text>A local fork of the pugixml project. This is a library to read XML files.</text>
					</description>
					<projects>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/pugixml</location>
						</external-project-link>
					</projects>
				</codesmithy-project>
				<codesmithy-project>
					<name>libgit2</name>
					<type>CodeSmithy.Group</type>
					<description>
						<text>A local fork of the libgit2 project. This is a client library to work with git repositories.</text>
					</description>
					<projects>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/libgit2</location>
						</external-project-link>
					</projects>
				</codesmithy-project>
				<codesmithy-project>
					<name>Ishiko Dependencies</name>
					<type>CodeSmithy.Group</type>
					<description>
						<text>The Ishiko-Cpp projects that CodeSmithy depends on. Each project in this group is a local fork of the official Ishiko-Cpp project.</text>
					</description>
					<projects>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/Errors</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/Process</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/FileTypes</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/WindowsRegistry</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/TestFramework</location>
						</external-project-link>
					</projects>
				</codesmithy-project>
				<codesmithy-project>
					<name>wxWidgets Dependencies</name>
					<type>CodeSmithy.Group</type>
					<description>
						<text>The wxWidgets code and its dependencies. These are local forks of the official wxWidgets projects.</text>
					</description>
					<projects>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/zlib</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/libpng</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/libexpat</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/libjpeg-turbo</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/libtiff</location>
						</external-project-link>
						<external-project-link>
							<location>https://github.com/CodeSmithyIDE/wxWidgets</location>
						</external-project-link>
					</projects>
				</codesmithy-project>
				<external-project-link>
					<location>https://github.com/CodeSmithyIDE/CodeSmithy</location>
				</external-project-link>
			</projects>
		</codesmithy-project>
	</projects>
</codesmithy-project-repository>
